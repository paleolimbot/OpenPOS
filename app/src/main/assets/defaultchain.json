{"type": "list",
"actions": [
  {
    "type": "vibrate"
  },
  {
    "type": "switch",
    "is_regex": true,
    "key": "gtin13",
    "values": ["^97.*?$", "^.*?$"],
    "actions": [
      {
        "type": "list",
        "name": "ISBN Chain",
        "actions": [
          {
            "type": "lookup",
            "name": "goodreads",
            "attribution": "http://www.goodreads.com/",
            "api_type": "REST",
            "uri_format": "https://www.goodreads.com/search/index.xml?key=HSkIMuOGlxFIOmfBCGFVA&q={{isbn13}}",
            "mime_type": "text/xml",
            "key_map": {
              "search/results/work/best_book/title": "title",
              "search/results/work/best_book/author/name": "authors"
            }
          },
          {
            "type": "stringformat",
            "key_map": {
              "amazon_search": "https://www.amazon.com/s/field-keywords={{isbn13}}",
              "worldcat": "http://www.worldcatlibraries.org/registry/gateway?isbn={{isbn13}}",
              "_description": "{{title}} ({{authors}})"
            }
          }
        ]
      },
      {
        "type": "lookup",
        "name": "sem3",
        "api_type": "REST",
        "uri_format": "https://api.semantics3.com/test/v1/products?q=%7B%22ean%22:%22{{gtin13}}%22%7D",
        "attribution": "http://www.semantics3.com/",
        "mime_type": "application/json",
        "header": {
          "api_key": "SEM3FC8D0BF4539905ECBF48230FD785747C"
        },
        "key_map": {
          "results[0]/name": "_description",
          "results[0]/description": "description",
          "results[0]/price": "price",
          "results[0]/manufacturer": "manufacturer",
          "results[0]/brand": "brand",
          "results[0]/model": "model",
          "message": "lookup_status"
        }
      }
    ]
  }
]}